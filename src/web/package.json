{
  "name": "myLambdaWebsite",
  "description": "My lambda website",
  "author": "Vazquez Nanini Ltd",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "scripts": {
    "postinstall": "./node_modules/prepush/bin/cli.js install",
    "devserver": "NODE_ENV=development ./node_modules/.bin/babel-watch --presets es2015 ./src/server/server.dev.js",
    "start-server": "node start.js",
    "spinup": "node ./src/CRA_hacks/buildProductionBundle && echo 'Starting production server' && yarn start-server",
    "start": "./start-local.sh",
    "start-local": "npm install && ./node_modules/.bin/concurrently \"npm run devserver\" \"node scripts/start.js\"",
    "build": "node ./src/CRA_hacks/buildProductionBundle",
    "test": "node scripts/test.js",
    "test-server": "./node_modules/mocha/bin/mocha src/server/**/*.spec.js",
    "test-coverage": "node scripts/test.js --coverage",
    "test-ci": "npm run eslint && ./node_modules/.bin/jest",
    "eslint": "./node_modules/.bin/eslint ./src --max-warnings 0",
    "eslint-fix": "./node_modules/.bin/eslint ./src --fix",
    "stylelint": "./node_modules/.bin/stylelint ./src/**/*.scss",
    "linters": "yarn eslint && yarn stylelint",
    "eslint-check-rules": "./node_modules/.bin/eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "src/**/*.js": [
      "precommit-flow-eslint"
    ],
    "src/**/*.scss": "precommit-stylelint"
  },
  "prepush": {
    "tasks": [
      "npm run eslint"
    ],
    "onDirtyState": "stash",
    "coloredOuput": true
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "aws-serverless-express": "^3.0.0",
    "axios": "0.17.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "babel-watch": "^2.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.1.0",
    "connect-history-api-fallback": "^1.4.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.0.5",
    "css-mq-splitter-plugin": "^1.0.0",
    "deep-freeze": "0.0.1",
    "detect-port": "^1.2.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.4",
    "esdoc": "^1.0.3",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "expose-loader": "^0.7.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "filesize": "^3.5.11",
    "find-cache-dir": "^1.0.0",
    "fs-extra": "^4.0.2",
    "gzip-size": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "jest": "^21.2.1",
    "jest-css-modules-transform": "^1.0.2",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^4.2.3",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "node-sass": "^4.3.0",
    "object-assign": "^4.1.1",
    "path-exists": "^3.0.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "prepush": "^3.1.11",
    "prettier": "1.9.2",
    "promise": "^8.0.1",
    "prop-types": "^15.5.8",
    "react-dev-utils": "^3.0.2",
    "react-error-overlay": "^1.0.9",
    "recompose": "^0.26.0",
    "recursive-readdir": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.4.0",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "scss-to-json": "^1.1.0",
    "sinon": "^4.0.2",
    "sinon-stub-promise": "^4.0.0",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "supertest": "^3.0.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "2.0.3",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "babel-preset-react-app": "0.2.1",
    "body-parser": "^1.18.2",
    "bootstrap-sass": "^3.3.7",
    "enzyme-adapter-react-16": "^1.0.2",
    "express": "^4.14.0",
    "fetch-everywhere": "^1.0.5",
    "flow": "^0.2.3",
    "flow-bin": "^0.57.3",
    "htmlparser2": "^3.9.2",
    "jquery": "^3.2.1",
    "lodash.debounce": "^4.0.8",
    "postcss": "^6.0.13",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-easy-swipe": "0.0.13",
    "react-gpt": "^2.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-retina-image": "^2.0.3",
    "react-router-dom": "^4.2.2",
    "react-share": "^2.0.0",
    "react-test-renderer": "16",
    "react-transition-group": "^2.2.1",
    "redux": "^3.6.0",
    "redux-form": "^7.2.0",
    "redux-saga": "^0.16.0",
    "serialize-javascript": "^4.0.0",
    "slugify": "1.2.9",
    "svg4everybody": "^2.1.8",
    "url-search-params-polyfill": "^2.0.3",
    "xmlbuilder": "^9.0.7",
    "yarn": "^1.2.1"
  },
  "babel": {
    "presets": [
      "react-app",
      "flow"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/src/reducers/",
      "/src/api/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform",
      ".+\\.(svg)$": "<rootDir>/config/jest/fileTransform"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "config",
      "<rootDir>/src/actions",
      "<rootDir>/src/components",
      "<rootDir>/src/themes/defaults/sass"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "setupTestFrameworkScriptFile": "./config/jest/setupTests.js"
  }
}
